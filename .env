# SEO Configuration
MIN_DESCRIPTION_LENGTH=100
MAX_DESCRIPTION_LENGTH=5000
MAX_KEYWORDS=20
KEYWORD_MIN_LENGTH=3

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_INTERVAL=60
SCRAPE_DELAY=2

# Cache Configuration
CACHE_ENABLED=true
CACHE_TYPE=redis
CACHE_EXPIRATION=3600
